<html>
    <head>
    <script src ="https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
    </head>

    <body> 
        <ol id = cakes></ol>
            <script type = "text/javascript">
                $.getJSON("/cake.json", function (data) {
                    $.each(data, function(key, val) {
                        $(cakes).append("<li>" + val + "</li>");
                    });
                });
            </script>
    </body>

</html>